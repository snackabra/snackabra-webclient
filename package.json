{
  "name": "snackabra-webclient",
  "author": "3384 Inc.",
  "email": "info@384.co",
  "description": "Snackabra web app (React Native)",
  "version": "0.6.11-beta.9",
  "bugs": "https://github.com/snackabra/snackabra-webclient/issues",
  "homepage": "https://snackabra.io",
  "license": "GPL-3.0-only",
  "private": false,
  "scripts": {
    "start": "GENERATE_SOURCEMAP=true react-app-rewired start",
    "bump": "VERSION=$(node -p \"require('./package.json').version\") &&  NEW_VERSION=$(echo $VERSION | awk -F'-' 'BEGIN{OFS=\"-\"} $2~/beta/{split($2, a, \".\"); a[2]++; $2=a[1]\".\"a[2]}1') && echo $NEW_VERSION && yarn version --new-version $NEW_VERSION",
    "deploy:staging": "yarn bump && yarn build && wrangler pages publish build --project-name 384chat --branch preview",
    "build": "GENERATE_SOURCEMAP=true react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "clean": "rm -rf build && rm -rf dist && rm -rf node_modules && rm yarn.lock",
    "extract": "lingui extract",
    "compile": "lingui compile"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": "off",
      "no-useless-escape": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.2",
    "@react-spring/web": "^9.6.1",
    "@terran-one/react-diff-viewer": "^3.1.2",
    "@types/node": "^18.11.18",
    "@use-gesture/react": "^10.2.24",
    "core-js": "^2.6.12",
    "file-selector": "^0.6.0",
    "mobx": "^6.6.2",
    "mobx-react": "^7.5.3",
    "mui-image": "^1.0.5",
    "react": "^18.2.0",
    "react-app-rewired": "2.1.3",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-native": "0.71.8",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-web": "0.19.4",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-swipeable-views": "^0.14.0",
    "snackabra": "^1.1.25",
    "styled-components": "^5.3.5",
    "ts-node": "^10.9.1",
    "workbox-core": "^6.5.3",
    "workbox-expiration": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "@lingui/cli": "^3.12.1",
    "@lingui/macro": "^3.12.1",
    "@lingui/react": "^3.12.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.5",
    "@types/react-native": "^0.69.0",
    "babel-loader": "8.0.6",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.23.0",
    "prop-types": "^15.8.1",
    "typescript": "4.6.2"
  }
}
